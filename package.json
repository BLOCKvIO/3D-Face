{
  "name": "@blockv/3d-face",
  "version": "2.0.3",
  "description": "This vAtom face can plug into the SDKs to render 3D content in either binary glTF or V3D format.",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build-lib && npm run copy-webapp && npm run copy-android",
    "build-lib": "webpack -p",
    "copy-webapp": "cpy lib/**/* webapp/",
    "copy-android": "cpy webapp/**/* android/face3d/src/main/assets/face3drenderer/"
  },
  "keywords": [
    "3d",
    "renderer",
    "v3d",
    "gltf",
    "vatom",
    "face"
  ],
  "author": "jjv360",
  "license": "ISC",
  "dependencies": {
    "@blockv/sdk": "^2.1.0",
    "hammerjs": "^2.0.8",
    "three": "^0.94.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.4",
    "cpy-cli": "^2.0.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2"
  }
}
